<dynamic_object>
	<diesel materials="units/pd2_mod_craft/material_mastercoff" orientation_object="rp_root" />
	<sequence_manager file="units/pd2_mod_craft/props/cake" />
	
	<bodies>
		<body name="body_static" enabled="true" template="static">
			<object collision_type="mesh_mopp" name="cake_side" />
			<object collision_type="mesh_mopp" name="cake_top" />
			<object collision_type="mesh_mopp" name="cake_bottom" />
		</body>
	</bodies>
	
	<graphics>
		<graphic_group name="g_all" culling_object="co_culling" enabled="true">
			<object	name="cake_side"			enabled="true" shadow_caster="true"/>
			<object	name="cake_top"			enabled="true" shadow_caster="true"/>
			<object	name="cake_bottom"			enabled="true" shadow_caster="true"/>
			<object	name="g_contour"		enabled="true" shadow_caster="false"/>
		</graphic_group>
	</graphics>
</dynamic_object>
