			<object name="g_g" collision_type="box" padding="-2.5"/>
		</body>
		<body name="fence_right_two" enabled="true" template="static">
			<object name="g_fence_right_two" collision_type="box" padding="-2.5"/>
		</body>
	    <body name="fence_right_one" enabled="true" template="static">
			<object name="g_fence_right_one" collision_type="box" padding="-2.5"/>
