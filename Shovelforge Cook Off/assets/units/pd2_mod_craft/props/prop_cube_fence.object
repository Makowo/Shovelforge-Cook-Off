	</bodies>
	
	<graphics>
		<graphic_group name="graphics" culling_object="g_g" enabled="true">
			<object	name="g_g"	enabled="true" shadow_caster="true"/>
		</graphic_group>
		<graphic_group name="fence_right_one" culling_object="g_fence_right_one" enabled="true">
			<object	name="g_fence_right_one"	enabled="true" shadow_caster="true"/>
		</graphic_group>
		<graphic_group name="fence_right_two" culling_object="g_fence_right_two" enabled="true">
			<object	name="g_fence_right_two"	enabled="true" shadow_caster="true"/>
		</graphic_group>
	</graphics>
