<dynamic_object>
	<diesel materials="units/pd2_mod_craft/material_mastercoff" orientation_object="rp_root" />
	
	<bodies>
		<body name="static_body" enabled="true" template="static">
			<object collision_type="mesh_mopp" name="g_log_side" />
			<object collision_type="mesh_mopp" name="g_log_top" />
		</body>
		<body name="foliage" enabled="true" template="static">
			<object collision_type="mesh_mopp" name="g_leaves" />
		</body>
	</bodies>
	
	<graphics>
		<graphic_group name="g_all" culling_object="co_culling" enabled="true">
			<object	name="g_log_side"			enabled="true" shadow_caster="true"/>
			<object	name="g_log_top"			enabled="true" shadow_caster="true"/>
			<object	name="g_leaves"				enabled="true" shadow_caster="true"/>
		</graphic_group>
	</graphics>
</dynamic_object>
